" colorscheme pablo
" colorscheme hybrid
colorscheme molokai
set background=dark
syntax enable
set guioptions-=b
set guioptions-=e
set guioptions-=T
set guioptions-=t
set guioptions-=L
set guioptions-=l
set guioptions-=R
set guioptions-=r
" set guifont=Ricty\ with-icons\ Regular:h14
set guifont=Ricty\ with-icons:h16
set showtabline=2
set number
set cursorline
" hi clear CursorLine
set vb t_vb=
set columns=120
set lines=60
set virtualedit=block

autocmd FocusGained * set transparency=250
autocmd FocusLost * set transparency=220
